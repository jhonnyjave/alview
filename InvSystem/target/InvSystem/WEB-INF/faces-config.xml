<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">
	<application>
		<navigation-rule>
			<navigation-case>
				<from-outcome>inicio</from-outcome>
				<to-view-id>faces/page/index.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-outcome>content</from-outcome>
				<to-view-id>faces/page/content.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-outcome>content2</from-outcome>
				<to-view-id>faces/page/content2.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-outcome>content3</from-outcome>
				<to-view-id>faces/page/content3.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
	</application>
</faces-config>
	